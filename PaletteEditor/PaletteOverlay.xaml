<UserControl x:Class="PaletteEditor.PaletteOverlay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" SnapsToDevicePixels="True"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Width="16" Height="16">
        <Border BorderThickness="1" BorderBrush="Blue" Width="16" Height="16" />
        <Border BorderThickness="1" BorderBrush="Blue" Width="18" Height="18" Margin="-1" />
    </Grid>
</UserControl>
